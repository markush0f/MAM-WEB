---
import Layout from "../layouts/Layout.astro";
import MAMInfo from "../components/MAMInfo.astro";
import Navbar from "../components/navbar.astro";
const webName = "MAM";
---

<Layout title="MAM-WEB">
  <Navbar />
  <div class="flex flex-col place-items-center justify-center">
    <img src="/MAM.png" alt="mam-logo" class="w-52" />
    <h1 class="text-7xl font-extrabold mt-">
      Welcome to <span class="text-primary">MAM</span>
    </h1>
  </div>
  <MAMInfo />
</Layout>
